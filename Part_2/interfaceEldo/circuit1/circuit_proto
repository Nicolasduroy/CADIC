
.global 0 

.param X1=100 X2=0.1

R01 vin vout 'X1'
C01 vout 0 'X2'

V0 vin 0 dc 3.3

.dc data=list

.ac dec 10 100 1G data=list

.option hmax=1u
.tran 1u 1m data=list

.data list X1 X2
xxPARAMxx
.enddata

.printfile dc v(vout) file=dcResult

.printfile tran v(vout) file=tranResult

.printfile ac v(vout) file=acResult