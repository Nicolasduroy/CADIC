
.option compat
.lib model

.global 0 

.param W1=3u L1=1u Win=3u Lin=360n Wb=2u Lb=180n

C01 vout 0 5p

V0 vin 0 dc 0.7
V0 vinp 0 dc 0.7
V1 vdd 0 dc 1.8
V2 vb 0 dc 0.7

m1 n1 n1 vdd vdd CMOSP w='WM' l='LM'
m2 vout n1 vdd vdd CMOSP w='WM' l='LM'

m3 n1 vinp n2 n2 CMOSN w='Win' l='Lin'
m4 vout vinn n2 n2 CMOSN w='Win' l='Lin'  # D_G_S_S

m5 n2 vb 0 0 CMOSN w='Wb' l='Lb'

.dc data=list

.data list WM LM Win Lin Wb Lb
xxPARAMxx
.enddata

.printfile dc v(vout) file=dcResult